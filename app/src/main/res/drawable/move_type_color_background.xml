<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/moveTypeColor">
        <shape android:shape="rectangle">
            <size
                android:width="40dp"
                android:height="20dp" />
            <solid android:color="@color/bug" />
        </shape>
    </item>

    <item
        android:left="40dp"
        android:right="-40dp"
        android:top="-65dp"
        android:bottom="-40dp">
        <rotate android:fromDegrees="30">
            <shape android:shape="rectangle">
                <solid android:color="@color/darkGray" />
            </shape>
        </rotate>
    </item>

</layer-list>